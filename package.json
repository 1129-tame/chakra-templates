{
  "name": "chakra-templates",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hauptrolle/chakra-templates.git"
  },
  "author": "Achim Rolle <achimrolle@googlemail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:ts": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "screenshots:build": "tsc --esModuleInterop true createScreenshots/index.ts --outDir createScreenshots/.build",
    "screenshots:take": "yarn screenshots:build && node createScreenshots/.build/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn lint:ts && yarn test:ci"
    }
  },
  "dependencies": {
    "@bigheads/core": "^0.3.1",
    "@chakra-ui/icons": "^1.0.2",
    "@chakra-ui/react": "^1.1.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "add": "^2.0.6",
    "copy-to-clipboard": "^3.3.1",
    "directory-tree": "^2.2.5",
    "framer-motion": "^3.1.1",
    "next": "10.0.4",
    "nprogress": "^0.2.0",
    "re-resizable": "^6.9.0",
    "react": "17.0.1",
    "react-code-blocks": "^0.0.8",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/node": "^14.14.16",
    "@types/nprogress": "^0.2.0",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^17.0.0",
    "babel-jest": "^26.6.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^5.5.0",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.1.3"
  }
}
